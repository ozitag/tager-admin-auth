{{#if recaptchaEnabled}}
    {{#ifCond recaptchaVersion '2'}}
        <script type="text/javascript">
            var RECAPTCHA_TOKEN = null;

            document.querySelector('.form__button').setAttribute('disabled', true);

            var onCaptchaLoadCallback = function () {

                let disabledTimeout;
                const disabledTimer = setInterval(() => {
                    if (document.getElementById('recaptcha_html_element') && document.getElementById('recaptcha_html_element').querySelector('iframe')) {
                        disabledTimeout = setTimeout(() => {
                            document.querySelector('.form__button').removeAttribute('disabled');
                        }, 350);
                        clearInterval(disabledTimer);
                    }
                }, 50);

                grecaptcha.render('recaptcha_html_element', {
                    'sitekey': '{{recaptchaSiteKey}}',
                    {{#if recaptchaInvisible}}size: 'invisible',{{/if}}
                    callback: response => {
                        RECAPTCHA_TOKEN = response;
                    },
                    'expired-callback': () => {
                        grecaptcha.reset();
                        RECAPTCHA_TOKEN = null;
                    },
                    'error-callback': () => {
                        if (disabledTimer) {
                            clearInterval(disabledTimer);
                        }
                        if (disabledTimeout) {
                            clearTimeout(disabledTimeout);
                        }

                        {{#if recaptchaInvisible}}
                            const $node = document.querySelector('.form-field-captcha');
                            if ($node) {
                                $node.classList.add('error');
                                //$node.innerText = '{{#ifCond language
                                                               'RU'}}Ошибка инициализации reCAPTCHA{{else}}Failure initialize reCAPTCHA{{/ifCond}}';
                            }
                        {{/if}}

                        document.querySelector('.form__button').setAttribute('disabled', true);
                    }
                });
            };
        </script>
    {{/ifCond}}

    <script>
        const RECAPTCHA_SITE_KEY = '{{recaptchaSiteKey}}';
        const RECAPTCHA_VERSION = '{{recaptchaVersion}}';
            {{#ifCond recaptchaVersion '2'}}
            const RECAPTCHA_INVISIBLE = {{#if recaptchaInvisible}}true{{else}}false{{/if}};
            {{/ifCond}}
    </script>
{{/if}}

{{#ifCond recaptchaVersion '3'}}
    <script src="https://www.google.com/recaptcha/api.js?render={{recaptchaSiteKey}}"></script>
{{/ifCond}}

{{#ifCond recaptchaVersion '2'}}
    <script src="https://www.google.com/recaptcha/api.js?onload=onCaptchaLoadCallback&render=explicit&hl={{language}}"
    async defer ></script>
{{/ifCond}}

{{#if googleAuthEnabled}}
    <script>
        gapi.load('auth2', function () {
            auth2 = gapi.auth2.init({
                client_id: '{{googleAuthClientId}}.apps.googleusercontent.com',
                cookiepolicy: 'single_host_origin',
            });

            document.getElementById('googleAuthButton').addEventListener('click', () => LoginFormUI.startLoader());
            auth2.attachClickHandler(document.getElementById('googleAuthButton'), {}, function (googleUser) {
                const idToken = googleUser.getAuthResponse().id_token;
                LoginFormUI.submitGoogleToken(idToken);
            }, function (error) {
                LoginFormUI.stopLoader();
                if (error.error !== 'popup_closed_by_user') {
                    console.log('Google Auth Error: ', JSON.stringify(error, undefined, 2));
                }
            });
        });
    </script>
{{/if}}

