<!doctype html>
<html lang="{{#ifCond language 'RU'}}ru{{else}}en{{/ifCond}}">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>{{ title }}</title>

    <link rel="apple-touch-icon" sizes="180x180" href="{{basePath}}/favicon/apple-touch-icon.png"/>
    <link rel="icon" type="image/png" sizes="32x32" href="{{basePath}}/favicon/favicon-32x32.png"/>
    <link rel="icon" type="image/png" sizes="16x16" href="{{basePath}}/favicon/favicon-16x16.png"/>
    <link rel="mask-icon" href="{{basePath}}/favicon/safari-pinned-tab.svg" color="{{brandColor}}"/>

    <link rel="manifest" href="{{basePath}}/favicon/site.webmanifest"/>
    <meta name="msapplication-config" content="{{basePath}}/favicon/browserconfig.xml"/>

    <meta name="theme-color" content="{{brandColor}}">
    <meta name="msapplication-navbutton-color" content="{{brandColor}}">
    <meta name="apple-mobile-web-app-status-bar-style" content="{{brandColor}}">

    {{#if googleAuthEnabled}}
        <meta name="google-signin-client_id" content="{{googleAuthClientId}}.apps.googleusercontent.com">
    {{/if}}

    <script>
        const API_BASE_URL = '{{apiBaseUrl}}';
        const WEB_BASE_URL = '{{baseUrl}}';

        const accessToken = localStorage.getItem('admin_access_token')

        if (accessToken) {
            window.location.href = WEB_BASE_URL.startsWith('https://') || WEB_BASE_URL.startsWith('http://') ? WEB_BASE_URL : window.location.origin + WEB_BASE_URL;
        }
    </script>

    <link href="//fonts.googleapis.com/css2?family=Montserrat&display=swap" rel="stylesheet">
    <style>
        div {
            --brand-color: {{brandColor}}
        }
    </style>

    {{#if googleAuthEnabled}}
        <script src="https://apis.google.com/js/api:client.js"></script>
    {{/if}}

    <link rel="stylesheet" href="{{basePath}}/app.css"/>
    <script src="{{basePath}}/app.js"></script>
</head>
